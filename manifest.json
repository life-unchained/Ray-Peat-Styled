{
    "manifest_version": 3,
    "name": "Ray Peat Styled",
    "version": "1.0",
    "description": "Restores original css and js to raypeat.com",
    "icons": {
        "16": "hsray_16.png",
        "128": "hsray_128.png"
    },

    "content_scripts": [
        {
        "matches": ["*://raypeat.com/*"],
        "css": ["raypeat.css"]
        }
    ]
}